<h1>Spaces</h1>

<% Space.all.reverse.each do |space| %>
  <div id='space-box'>
      <a id='space-title-<%=space.id%>' href="/spaces/<%=space.id%>"><%=space.name%></a>
      <p id='space-description'>
        <%=space.description%>
      </p>
      <p id='dates'>
        <span id='start-date'>Start date: <%=space.start_date%> </span>
        <span id='end-date'>End date: <%=space.end_date%> </span>
      </p>
  </div>
<% end %>
